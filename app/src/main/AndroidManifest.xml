<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jaysh.earthfull">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/earht_full"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogInHome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUp"
            android:parentActivityName=".LogInHome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".volunteer.VolunteerHome" />
        <activity
            android:name=".volunteer.Search"
            android:parentActivityName=".volunteer.VolunteerHome"></activity>
        <activity android:name=".volunteer.EventDetails"></activity>
    </application>

</manifest>